<%=form_for @place do |f| %>

<div class="form">
	<%=f.label :name %>
	<%=f.text_field :name %>
</div>

<div class="form">
	<%=f.label :food_type %>
	<%=f.text_field :food_type %>
</div>

<div class="form">
	Price (1-5):
	<%=f.text_field :price %>
</div>

<div class="form">
	<%=f.label :image_url %>
	<%=f.text_field :image_url %>
</div>

<div class="form">
	<%=f.label :website %>
	<%=f.text_field :website %>
</div>

<div class="form">
	<%=f.label :hours %>
	<%=f.text_field :hours %>
</div>


	<%=f.submit %>
<% end %>